{
  "name": "plaisio/exception-handler-core",
  "description": "PhpPlaisio: Core Exception Handler",
  "keywords": [
    "PhpPlaisio",
    "Exception Handler"
  ],
  "license": "MIT",
  "require": {
    "php": ">=8.3",
    "ext-dom": "*",
    "ext-libxml": "*",
    "plaisio/console": "^3.2.0",
    "plaisio/error-logger": "^1.4.0",
    "plaisio/exception": "^1.3.0",
    "plaisio/exception-handler": "^2.2.0",
    "plaisio/kernel": "^3.3.1",
    "plaisio/obfuscator": "^2.2.0",
    "plaisio/request": "^2.0.0",
    "plaisio/request-logger": "^1.4.0",
    "plaisio/response-core": "^2.9.0",
    "plaisio/session": "^5.1.0",
    "setbased/helper-code-store-php": "^2.8.0",
    "setbased/php-stratum-middle": "^5.14.0",
    "symfony/console": "^6.0|^7.0"
  },
  "require-dev": {
    "phing/phing": "^3.0.1",
    "phpunit/phpunit": "^11.5.25",
    "plaisio/console-kernel": "^1.2.0"
  },
  "autoload": {
    "psr-4": {
      "Plaisio\\ExceptionHandler\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Plaisio\\ExceptionHandler\\Test\\": "test/"
    }
  },
  "config": {
    "bin-dir": "bin/",
    "sort-packages": true,
    "allow-plugins": {
      "phing/phing-composer-configurator": true
    }
  }
}
