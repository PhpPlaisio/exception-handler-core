{
  "name": "plaisio/exception-handler-core",
  "description": "PhpPlaisio: Core Exception Handler",
  "keywords": [
    "PhpPlaisio",
    "Exception Handler"
  ],
  "license": "MIT",
  "require": {
    "php": ">=7.3",
    "ext-dom": "*",
    "ext-libxml": "*",
    "plaisio/console": "^2.0.1",
    "plaisio/error-logger": "^1.2",
    "plaisio/exception": "^1.1",
    "plaisio/exception-handler": "^2.0",
    "plaisio/kernel": "^3.1.1",
    "plaisio/obfuscator": "^2.0",
    "plaisio/request": "^0.12.1",
    "plaisio/request-logger": "^1.2",
    "plaisio/response-core": "^2.0",
    "plaisio/session": "^4.0",
    "setbased/helper-code-store-php": "^2.4.1",
    "setbased/php-stratum-middle": "^5.2",
    "symfony/console": "^5.0"
  },
  "require-dev": {
    "phing/phing": "^2.0",
    "phpunit/phpunit": "^9.0"
  },
  "autoload": {
    "psr-4": {
      "Plaisio\\ExceptionHandler\\": "src/"
    }
  },
  "config": {
    "bin-dir": "bin/",
    "sort-packages": true
  }
}
