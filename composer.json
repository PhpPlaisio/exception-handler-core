{
  "name": "plaisio/exception-handler-core",
  "description": "PhpPlaisio: Core Exception Handler",
  "keywords": [
    "PhpPlaisio",
    "Exception Handler"
  ],
  "license": "MIT",
  "require": {
    "php": ">=7.3",
    "ext-dom": "*",
    "ext-libxml": "*",
    "composer/composer": "^1.0",
    "plaisio/console": "^0.2.10",
    "plaisio/exception": "^1.1",
    "plaisio/exception-handler": "^1.1",
    "plaisio/kernel": "^1.1",
    "plaisio/response-core": "^1.1",
    "setbased/helper-code-store-php": "^2.3",
    "setbased/php-stratum-middle": "^5.2",
    "symfony/console": "^5.0"
  },
  "require-dev": {
    "phing/phing": "^2.0",
    "phpunit/phpunit": "^9.0",
    "plaisio/console": "^0.2.3"
  },
  "autoload": {
    "psr-4": {
      "Plaisio\\ExceptionHandler\\": "src/"
    }
  },
  "config": {
    "bin-dir": "bin/",
    "sort-packages": true
  }
}
